{
  "name": "project-venom",
  "version": "0.0.1",
  "description": "Project VENOM",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "build:dev": "webpack-dev-server --config \"./webpack/webpack.config.babel.js\" --open --env development",
    "build:prod": "webpack --config \"./webpack/webpack.config.babel.js\" --env production",
    "lint:js": "eslint --color ./client/",
    "lint:js-fix": "eslint --color --fix ./client/",
    "lint:js-check": "eslint --print-config . | eslint-config-prettier-check",
    "lint:css": "stylelint --color \"./client/**/*.js\"",
    "test": "jest --color --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx, json}": ["eslint --color --max-warnings=0", "stylelint --color", "prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slepets-a/project-venom.git"
  },
  "author": "Dmitriy Petrenko && Artem Slepets",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slepets-a/project-venom/issues"
  },
  "homepage": "https://github.com/slepets-a/project-venom#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel": "^6.23.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^1.1.2",
    "image-webpack-loader": "^4.4.0",
    "jest": "^23.6.0",
    "jest-runner-eslint": "^0.6.0",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-dev-utils": "^6.0.4",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.5.2",
    "redux-devtools": "^3.4.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
